        <div class="row">
        <% 1.upto(10) do |i|%>
            <div class="col s4">
              <div class="card small" style="width:100%; padding-right: 5px; padding-left: 5px;">
                <div class="card-image">
                  <img src="/assets/<%= ["da_main", "grand_main", "juwerly_main", "mega_main", "regen_main"].sample(1).first %>.jpg" style="height: 150px;">
